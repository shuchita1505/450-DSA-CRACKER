//Time Complexity: O(n). 
//Space Complexity: O(1). 

//{ Driver Code Starts
#include<bits/stdc++.h>
using namespace std;



// } Driver Code Ends
class Solution
{
    public:
    void sort012(int a[], int n)
    {int i;
        int x=0,b=0,c=0;
        for( i=0;i<n;i++)
{if(a[i]==0)
x++;
else if(a[i]==1)
b++;
else if(a[i]==2)
c++;}
    

int k=0;
  for( i=0;i<x;i++)
    a[k++]=0;
for( i=0;i<b;i++)
a[k++]=1;
for( i=0;i<c;i++)
a[k++]=2;

// code here 
}
    
};

//{ Driver Code Starts.
int main() {

    int t;
    cin >> t;

    while(t--){
        int n;
        cin >>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin >> a[i];
        }

        Solution ob;
        ob.sort012(a, n);

        for(int i=0;i<n;i++){
            cout << a[i]  << " ";
        }

        cout << endl;
        
        
    }
    return 0;
}


// } Driver Code Ends
